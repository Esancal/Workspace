		int num2 = 3;
		int num3 = 5;
		int num4 = 7;
		int num5 = 9;
		int num6 = 11;
		int num7 = 13;
		int num8 = 15;
		int num9 = 17;
		int num10 = 19;
		int num11 = 21;
		int num12 = 23;
		int num13 = 25;
		int num14 = 27;
		int num15 = 29;
		int num16 = 31;
		int num17 = 33;
		int num18 = 35;
		int num19 = 37;
		int num20 = 39;
	
		int oddsAddition = num1 + num2 + num3 + num4 + num5 + num6 + num7 + num8 + num9 + num10;
		System.out.println ("La suma de los primeros 10 números impares es:" + oddsAddition);

		   }

		public static boolean isOdd (int número) {
		return número % 2 == 0;
		  }

