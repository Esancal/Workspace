package com.generation.f20220525;
import java.util.Scanner;
public class CalculadoraComisiones {

	public static void main(String[] args) {
      	
        Scanner entrada = new Scanner(System.in);
      	
      	float comision1= 0.3;
        float comision2= 0.2;
        float comision3= 0.1;
		
      	int venta;
        double valorResultante;
          


  		venta = Integer.parseInt(entrada.nextLine());
		if(venta>=10000){
          
          valorResultante=(venta*comision1);
          System.out.println("La comision de "+venta+" es de: "+valorResultante);
        }else if(venta =>5001 && <=9999 ){
          
         	valorResultante=(venta*comision2);
      		System.out.println("La comision de "+venta+" es de: "+valorResultante);
          
        }else if(venta =>1001 && <=4999){
          
          	valorResultante=(venta*comision3);
          	System.out.println("La comision de "+venta+" es de: "+valorResultante);
          
          
        }else{
            System.out.println("Su venta no aplica para comision");
        }
		
          
	}
  		

}
